<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="struts-member"  extends="struts-pub" namespace="/member">
		<action name="*_*" method="{1}" class="{2}">
			<!-- 群管理初始化页面 -->
			<result name="memberInit">/WEB-INF/views/group/membermgr/memberInit.jsp</result>
			
			<!-- 群详细信息页面 -->
			<result name="memberDetailInfo">/WEB-INF/views/group/membermgr/memberDetailInfo.jsp</result>
		</action>
	</package>
</struts>